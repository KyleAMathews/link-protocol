datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Message {
  messageId String @unique
  channelId String
  guildId   String
  content   String

  createdTimestamp BigInt
  editedTimestamp  BigInt?

  links     String?
  reactions String?

  @@index([guildId])
}
